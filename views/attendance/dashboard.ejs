    <title>Attendance</title>
</head>
<body>
    <div class="container dash-container">
        <h1>Attendance</h1>
        </div>
    

    <div class="container">
        <form action="/clocking" method="post" class="col s12">
            <div class="row">
                <div class="input-field col-md-6 offset-md-3">
                    <label for="id">Enter Student Id</label><br>
                    <input type="number" id="id" name="empId">
                    
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 offset-md-5">
                    <input type="submit" name="clock" value="in" class='btn '>
                    <input type="submit" name="clock" value="out" class='btn red '>
                </div>
            </div>
        </form>
    </div>
    
    
</body>
</html>